function _init()
	t=0
	p_ani={21,22,23,24}
	_upd=update_game
	_drw=draw_game
	start_game()
end

function _update()
	t+=1
	_upd()
end

function _draw()
	_drw()
end

function start_game()
	p_x=2
	p_y=5
end

